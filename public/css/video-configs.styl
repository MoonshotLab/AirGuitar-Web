minVal = 16.6
minH = 25
minW = 16.6

#video-configs
  position absolute
  width 1000px
  height 700px
  z-index 1

  .config
    display none
    position absolute
    width 100%
    height 100%

  .mask
    top 0px
    left 0px
    position absolute
    width 100%
    height 100%
    overflow hidden
    -webkit-mask-position -35px -86px
    -webkit-mask-size 115.5%

  video
    -webkit-filter brightness(2)
    position absolute
    margin 0

  .config.takeover
    .mask
      width 100%
      height 100%
      -webkit-mask-size 102%
      -webkit-mask-position 6px 34px
      -webkit-mask-image url('/masks/showcase.svg')

  .config.two-up
    .mask:nth-child(1)
      -webkit-mask-image url('/masks/two-up-0.svg')
      video
        top -30px
        left 10px
        width 250px

    .mask:nth-child(2)
      -webkit-mask-image url('/masks/two-up-1.svg')
      video
        top -30px
        left 154px
        width 230px

    .mask:nth-child(3)
      -webkit-mask-image url('/masks/two-up-2.svg')
      video
        top -30px
        left 320px
        width 230px

    .mask:nth-child(4)
      -webkit-mask-image url('/masks/two-up-3.svg')
      video
        top 120px
        width 750px
        left -80px

    .mask:nth-child(5)
      -webkit-mask-image url('/masks/two-up-4.svg')
      video
        top 0
        left 290px
        width 700px

    .mask:nth-child(6)
      -webkit-mask-image url('/masks/two-up-5.svg')
      video
        left 450px
        top 480px
        width 220px

    .mask:nth-child(7)
      -webkit-mask-image url('/masks/two-up-6.svg')
      video
        left 580px
        top 480px
        width 250px

    .mask:nth-child(8)
      -webkit-mask-image url('/masks/two-up-7.svg')
      video
        left 775px
        top 480px
        width 220px

